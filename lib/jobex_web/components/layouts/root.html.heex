            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              stroke-width="2"
              d="M4 6h16M4 12h16M4 18h16"
            >
            </path>
          </svg>
        </button>
      </div>
      <div class="flex-1">
        <a class="text-xl btn btn-ghost">daisyUI</a>
      </div>
      <div class="flex-none">
